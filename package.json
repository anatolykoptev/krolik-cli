{
  "name": "@anatolykoptev/krolik-cli",
  "version": "0.18.0",
  "description": "Krolik Felix â€” Multi-Tier AI Orchestration for cost-optimized development",
  "keywords": [
    "cli",
    "ai",
    "developer-tools",
    "typescript",
    "code-review",
    "prisma",
    "trpc",
    "nextjs",
    "krolik"
  ],
  "author": "Anatoly Koptev",
  "license": "FSL-1.1-Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anatolykoptev/krolik-cli.git"
  },
  "homepage": "https://github.com/anatolykoptev/krolik-cli#readme",
  "bugs": {
    "url": "https://github.com/anatolykoptev/krolik-cli/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "krolik": "dist/bin/cli.js"
  },
  "files": [
    "dist",
    "claude-plugin",
    "scripts/postinstall.sh",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsx watch src/bin/cli.ts",
    "build": "pnpm sync-version && tsup",
    "krolik": "node dist/bin/cli.js",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "sync-version": "tsx scripts/sync-version.ts",
    "changeset": "changeset",
    "changeset:smart": "tsx scripts/smart-changeset.ts",
    "changeset:version": "changeset version && pnpm sync-version",
    "changeset:publish": "pnpm build && changeset publish",
    "prepublishOnly": "pnpm build",
    "postinstall": "bash scripts/postinstall.sh || true",
    "prepare": "husky"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@google/adk": "^0.3.0",
    "@google/genai": "^1.37.0",
    "@opentelemetry/api": "^1.9.0",
    "@swc/core": "^1.15.7",
    "@xenova/transformers": "^2.17.2",
    "better-sqlite3": "^12.5.0",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "cosmiconfig": "^9.0.0",
    "dotenv": "^17.2.3",
    "glob": "^13.0.0",
    "gpt-tokenizer": "^3.4.0",
    "i18next-cli": "^1.33.5",
    "minify-xml": "^4.5.2",
    "prettier": "^3.7.4",
    "sqlite-vec": "^0.1.6",
    "ts-morph": "^27.0.2",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.0",
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.15.0",
    "@vitest/coverage-v8": "^4.0.16",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "tsup": "^8.5.0",
    "tsx": "^4.20.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.0.0",
  "lint-staged": {
    "*.{ts,tsx}": [
      "biome check --write --no-errors-on-unmatched --diagnostic-level=error"
    ],
    "*.{json,md,yml,yaml}": [
      "biome format --write --no-errors-on-unmatched"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "better-sqlite3",
      "sharp",
      "protobufjs"
    ]
  }
}
